# My-captain-python
# to find the no. of each word in string

def most_frequent(string):
    d=dict()
    for key in string:
        if key not in d:
            d[key]=1
        else:
            d[key]+=1
    for i in d:
        print(i,":",d[i])
string=input("enter string=")
string=string.lower()
most_frequent(string)
